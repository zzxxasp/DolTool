        android:allowBackup="true"
        android:allowClearUserData="false"
        android:name="com.key.doltool.activity.MyApplication"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
		android:theme="@style/AppTheme" 
        android:screenOrientation="portrait"
        android:configChanges="keyboardHidden|orientation"
        >
        <meta-data android:name="Hadi_DB_NAME" android:value="demo.db" />
        <meta-data android:name="Hadi_DB_VERSION" android:value="1" />
        <!--申明-->
		<activity
		    android:name="net.youmi.android.AdBrowser"
		    android:configChanges="keyboard|keyboardHidden|orientation|screenSize"
		    android:theme="@android:style/Theme.Light.NoTitleBar" >
		</activity>
		<service
		    android:name="net.youmi.android.AdService"
		    android:exported="false">
		</service>
		<receiver
		    android:name="net.youmi.android.AdReceiver" >
		    <intent-filter>
		        <action android:name="android.intent.action.PACKAGE_ADDED" />
		        <data android:scheme="package" />
		    </intent-filter>
		</receiver>
		<activity
            android:name="com.key.doltool.activity.LoadingActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.Base.FullScreen"
            android:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>		
		<activity
            android:name="com.key.doltool.activity.core.MainActivity"
            android:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation"
